<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/css/bootstrap.min.css">
<script>"https://cdn.jsdelivr.net/npm/bootstrap@5.2.0-beta1/dist/js/bootstrap.bundle.min.js"</script>

<link rel="stylesheet" href="/index.css">
<nav>
    <h3>TestDemo</h3>
  <ul>
    {{!-- <li><a href="/">Home</a></li> --}}
    <li><a href="/login">Login</a></li>
    <li><a href="/register">Register</a></li>
  </ul>
</nav>
<div class="container mt-4 size">
  {{#if message }}
  <h4 class="alert alert-danger mt-4">{{message}}</h4>
  {{/if}}
  <div class="card">
    <div class="card-header">Registration Form</div>
    <div class="card-body">
      <form style="padding: 3px" action="/Developer" method="POST" enctype="multipart/form-data">
        <div>
          <label for="email" class="form-label">E-mail</label>
          <input type="email" class="form-control" name="email" required>
        </div>
        <div>
          <label for="password" class="form-label">Password</label>
          <input type="password" class="form-control" name="password" required>
        </div>
        <div>
          <label for="name" class="form-label">Name</label>
          <input type="text" class="form-control" name="name" id="name" >
        </div>
        <div>
          <label for="image" class="form-label">Choose a Picture</label>
          <input type="file" class="form-control" name="image" id="image">
        </div>
        <div>
          <label for="team_id" class="form-label">team_id</label>
          <input type="text" class="form-control" name="team_id">
        </div>
        <div>
          <label for="dob" class="form-label">Date of Birth</label>
          <input type="date" class="form-control" name="dob">
        </div>
        <button style="margin-top: 9px" type="submit" class="btn btn-primary b">Register Developer</button>
      </form>
    </div>
  </div>
</div>